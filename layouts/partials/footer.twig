<!--footer-->
<footer class="site-footer d-print-none">
    <div class="footer-top">
        <div class="container">        
            <div class="row">
                <div class="col-md-5 col-lg-3 icon-wrapper">
                    <a class="footer-brand" href="{{networkData.defaultBlogUrl}}" rel="Home" tabindex="-1" title="Home">
                        <img class="footer-logo__reversed" src="{{networkData.templatePath}}/static/images/newsroom-reversed2.png" alt="logo">
                    </a>
                    <span><a href="https://publish.aap.com.au">POWERED BY AAP PUBLISH</a></span>
                </div><!-- .col-sm-3-->
                <div class="col-md-7 col-lg-9 pull-right text-right logo-wrapper u-mobile-margin-bottom-50">
                    <img src="{{networkData.templatePath}}/static/images/nr-logo-reverse.svg" class="symbol">

                    <div class="footer-subscribe">
                        <h3 class="footer-subscribe__heading">Get your free daily briefing</h3>
                        <p class="footer-subscribe__tagline">Start your day with our best stories in your inbox</p>



                        <!-- Begin MailChimp Signup Form -->
                        <div id="mc_embed_signup" class="footer-subscribe__container" >
                            <form   action="//newsroom.us14.list-manage.com/subscribe/post?u=e0ae259e8f9472b9c54037c25&amp;id=71de5c4b35" 
                                    method="post" id="mc-embedded-subscribe-form" 
                                    name="mc-embedded-subscribe-form" 
                                    class="validate" 
                                    target="_blank" 
                                    novalidate 
                                    style="background-color:white">
                                <div id="mc_embed_signup_scroll" style="display:flex">
                            
                                    <div class="mc-field-group" style="">
                                    {# <label for="mce-EMAIL">Email Address </label> #}
                                        <input type="email" value="" name="EMAIL" class="footer-subscribe__email required " id="mce-EMAIL" placeholder="Enter your email" style="color:black; border:none">
                                        <input type="hidden" class="u-hide" value="1" name="group[3][1]" id="mce-group[3]-3-0">
                                    </div >
                                    <div id="mce-responses" class="clear">
                                        <div class="response" id="mce-error-response" style="display:none"></div>
                                        <div class="response" id="mce-success-response" style="display:none"></div>
                                    </div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                                    <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_e0ae259e8f9472b9c54037c25_71de5c4b35" tabindex="-1" value=""></div>
                                    <button type="submit" value=" " name="subscribe" id="mc-embedded-subscribe" class="footer-subscribe__submit tag-register-button"></button>
                                </div>
                            </form>
                        </div>








                      

                    </div>
                </div><!-- .col-sm-6-->
            </div><!--row-->

            <div class="row">
                <div class="col-md-12">
                    <div class="social-links">
                        <ul>

                            <li><a href="https://www.facebook.com/newsroomnz"><span class="sr-only">Facebook</span><span class="icon-facebook"></span></a></li>
                            <li><a href="https://twitter.com/NewsroomNZ"><span class="sr-only">Twitter</span><span class="icon-twitter"></span></a></li>
                            <li><a href="https://www.linkedin.com/company/newsroom-new-zealand"><span class="sr-only">Linkedin</span><span class="icon-linkedin"></span></a></li>
                        </ul>
                    </div><!-- .social-links-->
                </div><!-- .col-sm-12-->
            </div><!-- .row-->

            <div class="row">
                {# <div class="col-sm-12" style="">
                    <p style="color:white;font-size:12px">*Joint winner of Voyager Media Awards 'Website of the Year' with Stuff.co.nz</p><!-- .footer-menu -->
                </div><!-- .col-sm-12--> #}

                <div class="col-md-12" style="">
                    <div class="footer-menu">
                        <span><a href="https://publish.aap.com.au">POWERED BY AAP PUBLISH</a></span>
                    </div><!-- .footer-menu -->
                </div><!-- .col-sm-12-->
            </div><!-- .row-->
        </div><!-- .container-->
    </div><!-- .footer-top-->

    <div class="footer-btm">
        <div class="footer-menu">
            <ul>
                <li><a href="{{networkData.defaultBlogUrl}}/contact">Contact Us</a></li>
                <li><a href="{{networkData.defaultBlogUrl}}/terms-and-conditions">Terms and Conditions</a></li>
                <li><a href="{{networkData.defaultBlogUrl}}/privacy-policy">Privacy Policy</a></li>
                <li><a href="{{networkData.defaultBlogUrl}}/faq">Pro FAQ</a></li>
                <li><a href="{{networkData.defaultBlogUrl}}/media-council-new-zealand">New Zealand Media Council</a></li>
                
            </ul>
        </div><!-- .footer-menu -->
        <div class="rooms-link">
            <ul>
                {% set menuItems = headerMenu %}
                {% for i, parentItem in menuItems %}
                    {% set activeClass = (parentItem.link == _AppHelper.getServerUrl()) ? 'active' : ''%}
                    {% set externalClass = (parentItem.isExternal) ? 'external-link' : '' %}
                    
                    {% if (parentItem.blogTitle != "" and parentItem.blogTitle | lower != roomInfo[0] | lower ) %}
                        <li>
                            <a href="{{parentItem.link |raw}}" class="{{activeClass}} {{externalClass}}" {% if parentItem.target != "" %}target="{{parentItem.target}}"{% endif %}>
                                <img src="{{networkData.templatePath}}/static/images/{{parentItem.blogTitle | lower}}-logo.svg?v=572" alt="">
                            </a>
                        </li>
                    {% endif %}

                {% endfor %}

            </ul>
        </div>
    </div><!-- .footer-btm-->

<script>
if (document.getElementById('tagdiv')) {
    addClassTagListeners('tag-register-button','signup','click');
    addTagListeners('getprobutton','getpro','click');
    addTagListeners('learnmorebutton','learnmore','click');
}
</script>
    
</footer>
