<section class="subscribe form {{class}}">
    <img src="{{networkData.templatePath}}/static/images/newsroom-logo.svg?v=03" alt="newsroom wordmark">
    <h3>Register</h3>
    <p>To receive our free daily snapshot of<br /> news that matters to New Zealanders</p>
    

    <!-- Begin MailChimp Signup Form -->
    <div id="mc_embed_signup">
        <form action="//newsroom.us14.list-manage.com/subscribe/post?u=e0ae259e8f9472b9c54037c25&amp;id=71de5c4b35" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate subscribe-form" target="_blank" novalidate style="background-color:white">
            <div id="mc_embed_signup_scroll" style="width:100%;">
        
                <div class="mc-field-group input-group" >
                    <input type="email" value="" name="EMAIL" class="required email field form-control" id="mce-EMAIL" placeholder="Email address">
                    <input type="hidden" class="u-hide" value="1" name="group[3][1]" id="mce-group[3]-3-0">

                    <span class="input-group-btn">
                        <button class="btn btn-default tag-register-button" type="submit" value=" " name="subscribe" id="mc-embedded-subscribe" aria-label="Newsletter subscribe">
                            <span class="icon-right-small"></span>
                        </button>
                    </span>
                </div >

                <div id="mce-responses" class="clear">
                    <div class="response" id="mce-error-response" style="display:none"></div>
                    <div class="response" id="mce-success-response" style="display:none"></div>
                </div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_e0ae259e8f9472b9c54037c25_71de5c4b35" tabindex="-1" value=""></div>
            </div>
        </form>
    </div>




